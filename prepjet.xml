        <bt:Image id="PJ.trimspaces.Icon" DefaultValue="https://localhost:8443/images/trimspaces.png" />
        <bt:Image id="PJ.validationrule.Icon" DefaultValue="https://localhost:8443/images/validationrule.png" />
        <bt:Image id="PJ.addcolumn.Icon_sm" DefaultValue="https://localhost:8443/images/addcolumn_sm.png" />
        <bt:Image id="PJ.detectoutlier.Icon_sm" DefaultValue="https://localhost:8443/images/detectoutlier_sm.png" />
        <bt:Image id="PJ.removeduplicates.Icon_sm" DefaultValue="https://localhost:8443/images/removeduplicates_sm.png" />
        <bt:Image id="PJ.splitvalues.Icon_sm" DefaultValue="https://localhost:8443/images/splitvalues_sm.png" />
        <bt:Image id="PJ.extractvalues.Icon_sm" DefaultValue="https://localhost:8443/images/extractvalues_sm.png" />
        <bt:Image id="PJ.trimspaces.Icon_sm" DefaultValue="https://localhost:8443/images/trimspaces_sm.png" />
        <bt:Image id="PJ.validationrule.Icon_sm" DefaultValue="https://localhost:8443/images/validationrule_sm.png" />
        <bt:Image id="PJ.addcolumn.Icon_lg" DefaultValue="https://localhost:8443/images/addcolumn_lg.png" />
        <bt:Image id="PJ.detectoutlier.Icon_lg" DefaultValue="https://localhost:8443/images/detectoutlier_lg.png" />
        <bt:Image id="PJ.removeduplicates.Icon_lg" DefaultValue="https://localhost:8443/images/removeduplicates_lg.png" />
        <bt:Image id="PJ.splitvalues.Icon_lg" DefaultValue="https://localhost:8443/images/splitvalues_lg.png" />
        <bt:Image id="PJ.extractvalues.Icon_lg" DefaultValue="https://localhost:8443/images/extractvalues_lg.png" />
        <bt:Image id="PJ.trimspaces.Icon_lg" DefaultValue="https://localhost:8443/images/trimspaces_lg.png" />
        <bt:Image id="PJ.validationrule.Icon_lg" DefaultValue="https://localhost:8443/images/validationrule_lg.png" />

      </bt:Images>
      <bt:Urls>
        <bt:Url id="PJ.FunctionFile.Url" DefaultValue="https://commandsimple.azurewebsites.net/FunctionFile.html" />
        <bt:Url id="PJ.addcolumns.Url" DefaultValue="https://localhost:8443/app/home/merge_columns.html" />
        <bt:Url id="PJ.SplitValues.Url" DefaultValue="https://localhost:8443/app/home/split_values.html" />
        <bt:Url id="PJ.FindDuplicates.Url" DefaultValue="https://localhost:8443/app/home/duplicates.html" />
        <bt:Url id="PJ.DetectOutliers.Url" DefaultValue="https://localhost:8443/app/home/outlier.html" />
        <bt:Url id="PJ.ExtractValues.Url" DefaultValue="https://localhost:8443/app/home/extract_values.html" />
        <bt:Url id="PJ.TrimSpaces.Url" DefaultValue="https://localhost:8443/app/home/trim_spaces.html" />
        <bt:Url id="PJ.validation.Url" DefaultValue="https://localhost:8443/app/home/validation.html" />
        <bt:Url id="PJ.intro.Url" DefaultValue="https://localhost:8443/app/home/intro.html" />
        <!--LearnMore URL currently not used -->
        <bt:Url id="PJ.GetStarted.LearnMoreUrl" DefaultValue="http://www.prepjet.de/about-1/">
        </bt:Url>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="PJ.addcolumnButton.Label" DefaultValue="Add Columns" />
        <bt:String id="PJ.SplitValuesButton.Label" DefaultValue="Split Values" />
        <bt:String id="PJ.FindDuplicatesButton.Label" DefaultValue="Detect Duplicates" />
        <bt:String id="PJ.DetectOutliersButton.Label" DefaultValue="Detect Outliers" />
        <bt:String id="PJ.ExtractValuesButton.Label" DefaultValue="Extract Values" />
        <bt:String id="PJ.TrimSpacesButton.Label" DefaultValue="Trim Spaces" />
        <bt:String id="PJ.ValidationRuleButton.Label" DefaultValue="Validation Rule" />
        <bt:String id="PJ.IntroButton.Label" DefaultValue="PrepJet Help" />
        <bt:String id="PJ.Dropdown.Label" DefaultValue="Dropdown" />
        <bt:String id="PJ.Item1.Label" DefaultValue="Show Taskpane 1" />
        <bt:String id="PJ.Item2.Label" DefaultValue="Show Taskpane 2" />
        <bt:String id="PJ.Tab1.GroupLabel" DefaultValue=" " />
        <bt:String id="PJ.Tab1.GroupLabel2" DefaultValue=" " />
        <bt:String id="PJ.Tab1.TabLabel" DefaultValue="PrepJet" />
        <bt:String id="PJ.GetStarted.Title" DefaultValue="PrepJet was succesfully loaded">
          <bt:Override Locale="ja-jp" Value="JA-JP Get Started Title" />
        </bt:String>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="PJ.addcolumnButton.Tooltip" DefaultValue="Click to Execute Function" />
        <bt:String id="PJ.PJ.SVBtn.Ttp" DefaultValue="Click to Show a Taskpane" />
        <bt:String id="PJ.Dropdown.Tooltip" DefaultValue="Click to Show Options on this Menu" />
